{
  "name": "@arkadia/data",
  "version": "0.1.7",
  "description": "Parser and Stringifier for Arkadia Data Format (AKD)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "check": "prettier --check . && eslint .",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "verify": "node scripts/verify-build.js",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "ai",
    "data",
    "format",
    "parser",
    "schema",
    "arkadia"
  ],
  "author": "Arkadia Solutions",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.19.31",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^3.2.4"
  }
}
